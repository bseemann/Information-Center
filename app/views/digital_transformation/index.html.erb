<div id="form-container">
  <div id="conversion" class="wrapper">
    <header>
      <hgroup>
        <% if params.has_key?('landing') && params['landing'] == '1' %>
            <h1>Saiba como a AIESEC te dá suporte durante o seu intercâmbio</h1>
            <h2>Preencha esse formulário para ter acesso ao video explicativo</h2>
        <% elsif params.has_key?('landing') && params['landing'] == '2' %>
            <h1>15 dicas para planejar a sua viagem sem imprevistos</h1>
            <h2>Preencha esse formulário para ter acesso ao ebook explicativo</h2>
        <% end %>
      </hgroup>
    </header>

    <section>
      <%= form_for(:form_data ,:url => { :action => 'update' }) do |f| %>
          <% if params.has_key?('error') %>
            <p style="color: darkorange" class="notice">Certifique-se de que todos os campos foram selecionados e de que você está usando o mesmo e-mail que se cadastrou anteriormente no <a style="color: cornflowerblue" href="http://aiesec.org.br/inscricao">site da AIESEC</a></p>
          <% end %>
          <div class="field">
            <label for="email">E-mail cadastrado*</label>
            <%= f.text_field 'email', :class => 'form-control required' %>
          </div>
          <div class="field">
            <label for="programa_interesse">Programa de interesse *</label>
            <%= f.select 'programa_interesse', @interested_program_sub_product, :class => 'form-control required' %>
          </div>
          <div class="field">
            <label for="como_conheceu">Como você conheceu a AIESEC? *</label>
            <%= f.select 'como_conheceu', @how_got_to_know_aiesec, :class => 'form-control required' %>
          </div>
          <div class="field">
            <label for="entidade_proxima">Confirme a entidade da AIESEC mais próxima de você *</label>
            <%= f.select 'entidade_proxima', @entities, :class => 'form-control required' %>
          </div>
          <div class="actions">
            <% if params.has_key?('landing') && params['landing'] == '1' %>
                <%= f.submit 'Abrir video', :class => 'call_button' %>
            <% elsif params.has_key?('landing') && params['landing'] == '2' %>
                <%= f.submit 'Baixar ebook', :class => 'call_button' %>
            <% end %>
          </div>
          <p class="notice">Seu registro será enviado para a AIESEC mais próxima. Assim poderemos te ajudar a escolher seu intercâmbio</p>
          <input type="hidden" name="landing" value="<%= params['landing'] %>" />
    <% end %>
    </section>
  </div>
</div>